{
    "name": "jezzdk/statamic-google-maps",
    "authors": [
        {
            "name": "Jess Stopa"
        }
    ],
    "support": {
        "email": "jess@stopa.dk"
    },
    "autoload": {
        "psr-4": {
            "Jezzdk\\StatamicGoogleMaps\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^6.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Google Maps",
            "description": "Statamic Google Maps addon",
            "editions": [
                "free",
                "pro"
            ]
        },
        "laravel": {
            "providers": [
                "Jezzdk\\StatamicGoogleMaps\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
